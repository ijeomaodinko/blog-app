  <h1>Here are list of users</h1>
    <p>Users</p>
    <section>
    <% @users.each do |user| %>
    <div>
    <img class="user-image" src="<%= user.photo %>" alt="user photo">
    </div>
    <div>
     <h2><%= user.name %></h1>
     <h3>Number of posts: <%= user.posts_counter %></h3>
     </div>
     <% end %>
    </section>

